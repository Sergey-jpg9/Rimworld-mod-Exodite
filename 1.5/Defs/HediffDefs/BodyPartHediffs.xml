  <HediffDef>
    <defName>AeldariHediffGiver</defName>
    <label>service hediff</label>
    <description>Hidden hediff that installs new bodyparts. It will disappear later.</description>
    <hediffClass>HediffWithComps</hediffClass>
    <!--<battleStateLabel>blind</battleStateLabel>-->
    <maxSeverity>1</maxSeverity>
	<stages>
		<li>
			<becomeVisible>false</becomeVisible>
			<partEfficiencyOffset>0.2</partEfficiencyOffset>
		</li>
	</stages>
    <hediffGivers>
		<li Class="Exodites.HediffGiver_Momentum">
				<hediff>EldarEye</hediff>
				<partsToAffect>
					<li>Eye</li>
				</partsToAffect>
				<countToAffect>2</countToAffect>
			</li>
			
			<li Class="Exodites.HediffGiver_Momentum">
				<hediff>EldarLeg</hediff>
				<partsToAffect>
					<li>Leg</li>
				</partsToAffect>
				<countToAffect>2</countToAffect>
			</li>
			
			<li Class="Exodites.HediffGiver_Momentum">
				<hediff>EldarEar</hediff>
				<partsToAffect>
					<li>Ear</li>
				</partsToAffect>
				<countToAffect>2</countToAffect>
			</li>
	</hediffGivers>
    <comps>
      <li Class="HediffCompProperties_SelfHeal">
        <healAmount>0.2</healAmount>
      </li>
    </comps>
  </HediffDef>
  
